}
.debt{
  position:fixed;
  top:0;
  width:100%;
  z-index:99999999999;
}
.debts{
  position:fixed;
  top:0;
  width:100%;
}
.debt-pop{
  height:100vh;
  background:rgba(0,0,0,.5);
}
.debt-open{
  width:610rpx;
  /* height:300rpx; */
