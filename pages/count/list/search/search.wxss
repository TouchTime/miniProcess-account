/* 弹窗 */
.select-one{
  color:#333;
  font-size: 30rpx;
  height:100rpx;
  line-height:100rpx;
  text-align: center;
  border-bottom: 1rpx solid #e3e4e6;
}
.select-open{
  background:#fff;
  width:100%;
  position:fixed;
  bottom:0;

}
.select{
  position:fixed;
  top:0;
