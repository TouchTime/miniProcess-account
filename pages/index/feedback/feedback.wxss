@import "../problem-template/problem-template.wxss";
page{
  border-top:1rpx solid #cacbcc;
}
.container{
   display: flex;
  flex-direction:row;
}
.feedback-logo{
  width:110rpx;
  height:110rpx;
}
.feedback-text{
  font-size: 30rpx;
  color:#666666;
  margin-top:10rpx;
}
.first-content{
  width:33.3%;
  text-align: center;
  padding:30rpx 0;
  background:#fff;
}
.hot-problem{
  font-size: 30rpx;
  color:#999;
  padding:24rpx 0 24rpx 30rpx;
  background:#fff;
  margin-top:22rpx;
}
.problem-container{
  background:#fff;
  border-top: 1rpx solid #e3e4e6;
}